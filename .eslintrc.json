{
  "env": {
    "es2021": true,
    "node": true
  },
  "plugins": ["prettier"],
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "globals": {
    "use": true
  },
  "rules": {
    "prettier/prettier": "error",
    "strict": "off",
    "class-methods-use-this": "off",
    "no-param-reassign": "off"
  }
}
